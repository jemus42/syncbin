{
    "highlight_selected_line": true,
    "show_invisibles": true,
    "show_indent_guides": true,
    "tab_multiline_completion": true,
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "syntax_color_console": true,
    "scroll_past_end_of_document": true,
    "highlight_r_function_calls": true,
    "save_files_before_build": true,
    "font_size_points": 13,
    "editor_theme": "Monokai Spacegray Eighties",
    "default_encoding": "UTF-8",
    "default_project_location": "~/repos",
    "default_sweave_engine": "knitr",
    "default_latex_program": "XeLaTeX",
    "pdf_previewer": "rstudio",
    "insert_numbered_latex_sections": true,
    "document_author": "Lukas",
    "rmd_preferred_template_path": "/Users/Lukas/Library/R/shared/learnr/rmarkdown/templates/tutorial",
    "show_publish_diagnostics": true,
    "show_doc_outline_rmd": true,
    "show_rmd_render_command": true,
    "save_workspace": "never",
    "line_ending_conversion": "posix",
    "remove_history_duplicates": true,
    "doc_outline_show": "sections_and_chunks",
    "posix_terminal_shell": "custom",
    "custom_shell_command": "/usr/local/bin/zsh",
    "shiny_viewer_type": "pane",
    "margin_column": 90,
    "new_proj_git_init": true,
    "jobs_tab_visibility": "shown",
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true
    },
    "console_double_click_select": true,
    "style_diagnostics": true,
    "background_diagnostics_delay_ms": 1500,
    "use_tinytex": true,
    "terminal_bell_style": "none"
}